@model List<AutoMapperExample.DTO.UserViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Surname</th>
                <th>FullName</th>
                <th>Email</th>
                <th>FullAdress</th>
                <th>Country</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Surname</td>
                    <td>@item.FullName</td>
                    <td>@item.Email</td>
                    <td>@item.FullAdress</td>
                    <td>@item.Country</td>
                </tr>
            }
        </tbody>
    </table>
</div>
